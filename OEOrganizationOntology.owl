    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Agriculture -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Agriculture">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Agriculture: individuals and groups involved in the practices of crop cultivation
including government agencies whose primary concern is agricultural issues</rdfs:comment>
        <rdfs:label>Agriculture</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Business -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Business">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Business: businessmen, companies, and enterprises, not including MNCs</rdfs:comment>
        <rdfs:label>Business</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Civilian -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Civilian">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Civilian individuals or groups sometimes used as catch-all for individuals or
groups for whom no other role category is appropriate</rdfs:comment>
        <rdfs:label>Civilian</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Criminal -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Criminal">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Criminal: corresponding to individuals involved in or allegedly involved in the
deliberate breaking of state or international laws primarily for profit</rdfs:comment>
        <rdfs:label>Criminal</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Development -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Development">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Development: individuals or groups concerned primarily with development
issues of varying types including infrastructure creation, democratization et al.</rdfs:comment>
        <rdfs:label>Development</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Education -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Education">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Education: educators, schools, students, or organizations dealing with education</rdfs:comment>
        <rdfs:label>Education</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Elites -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Elites">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Elites: former government officials, celebrities, spokespersons for organizations
without further role categorization (George Soros, former Secretary of Defense, Bono)</rdfs:comment>
        <rdfs:label>Elites</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Environmental -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Environmental">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Environmental: entities for whom environmental and ecological issues are
their primary focus, includes wildlife preservation, climate change, etc.</rdfs:comment>
        <rdfs:label>Environmental</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Executive -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Executive">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Government"/>
        <rdfs:comment>Definition: Executive: Elected and non-elected officials and supporting organizations. The executive is the organ exercising authority in and holding responsibility for the governance of a state. The executive executes and enforces law.</rdfs:comment>
        <rdfs:label>Executive</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Health -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Health">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Health: individuals, groups and organizations dealing with health and social
welfare practices (doctors, Doctors Without Borders)</rdfs:comment>
        <rdfs:label>Health</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#HumanRights -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#HumanRights">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Human Rights: actors for whom their primary area of operation or expertise
is with documenting and/or correcting human rights concerns</rdfs:comment>
        <rdfs:label>Human Rights</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Insurgent -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Insurgent">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Rebels"/>
        <rdfs:comment>Definition: Insurgents (rebels): all rebels who attempt to overthrow their national government</rdfs:comment>
        <rdfs:label>Insurgent</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Judiciary -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Judiciary">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Government"/>
        <rdfs:comment>Definition: Judiciary: judges, courts</rdfs:comment>
        <rdfs:label>Judiciary</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Labor -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Labor">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Labor: specifcally individuals in or elements of organized labor, organizations
concerned with labor issues</rdfs:comment>
        <rdfs:label>Labor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Legislature -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Legislature">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Government"/>
        <rdfs:comment>Definition: Legislature: parliaments, assemblies, lawmakers, references to specific
legislative entities or sub-entities such as committees</rdfs:comment>
        <rdfs:label>Legislature</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Media -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Media">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Media: journalists, newspapers, television stations also includes providers of
internet services and other forms of mass information dissemination</rdfs:comment>
        <rdfs:label>Media</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Police -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Police">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Police forces, officers, criminal investigative units, protective agencies</rdfs:comment>
        <rdfs:label>Police</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#PoliticalOpposition -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#PoliticalOpposition">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Political opposition: opposition parties, individuals, anti-government activists</rdfs:comment>
        <rdfs:label>Political Opposition</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Rebels -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Rebels">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Rebels: armed and violent opposition groups, individuals</rdfs:comment>
        <rdfs:label>Rebels</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Refugees -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Refugees">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: Refugees: also refers to agencies or MNCs dealing with population migration
and relocation issues</rdfs:comment>
        <rdfs:label>Refugees</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Religious -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Religious">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/aggregate_has_quality"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Ethnicity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Definition: Organizations or groups specifically tied to a Religious philosopy.</rdfs:comment>
        <rdfs:label>Religious</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Separatist -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Separatist">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Rebels"/>
        <rdfs:comment>Definition: Separatist rebels: all rebels who try to emancipate their region from its country</rdfs:comment>
        <rdfs:label>Separatist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Spy -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#Spy">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Organization"/>
        <rdfs:comment>Definition: State intelligence services and members including covert operations groups
as well as intelligence collection and analyses</rdfs:comment>
        <rdfs:label>Spy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#UnalignedArmedForces -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEOrganizationOntology#UnalignedArmedForces">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ArmedForce"/>
        <rdfs:comment>Definition: Armed forces aligned neither with nor against their government</rdfs:comment>
        <rdfs:label>Unaligned Armed Force</rdfs:label>
    </owl:Class>
</rdf:RDF>




    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/DIMEFILOntology#DIMEFIL_Financial_Category_Value">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/DIMEFILOntology#DIMEFIL_Value"/>

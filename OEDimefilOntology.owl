    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/DIMEFILOntology#DIMEFIL_Economic_Category_Value">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/DIMEFILOntology#DIMEFIL_Value"/>
        <CommonCoreOntologies:alternative_label>DIMEFIL Economic Category Value</CommonCoreOntologies:alternative_label>

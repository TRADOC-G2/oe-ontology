        <obo:IAO_0000112>President Yoweri Museveni has banned Ugandan opposition candidates from participating in the upcoming elections.</obo:IAO_0000112>
        <obo:IAO_0000115>Prevent establishment or activities of political parties or politicians.    </obo:IAO_0000115>
        <obo:IAO_0000119>CAMEO Event and Actor Codebook Version: 1.1b3</obo:IAO_0000119>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ban political parties or politicians</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#CameoCode_1723 -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#CameoCode_1723">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/designates"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#Impose_curfew"/>

        <rdfs:comment>Code: 1241</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Refuse to ease administrative sanctions</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#Refuse_to_ease_economic_sanctions_boycott_or_embargo -->


        <obo:IAO_0000119>CAMEO Event and Actor Codebook Version: 1.1b3</obo:IAO_0000119>
        <rdfs:comment>Usage: Source must be the plaintiff or the state, and target must be the defendant.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bring lawsuit against</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#CameoCode_116 -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#CameoCode_116">
        <owl:equivalentClass>
            <owl:Restriction>

    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#CameoCode_112 -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#CameoCode_112">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/designates"/>

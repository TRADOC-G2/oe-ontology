        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/designates"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologylibrary.mil/OperationalEnvironment/Mid/OEEventOntology#Accuse_of_human_rights_abuses"/>
